---
layout: null
---
<?xml version="1.0" encoding="UTF-8" ?>
<rss version="2.0">
<channel>
 <title>IWW NYC GMB Newsletter</title>
 <link>{{ site.url }}</link>

 {% for post in site.categories.news %}
 <item>
  <title>{{ post.title }}</title>
  <link>{{ post.url }}</link>
  <guid isPermaLink="true">{{ site.url }}{{ post.url }}</guid>
  <pubDate>{{ post.date }}</pubDate>
 </item>
 {% endfor %}

</channel>
</rss>
